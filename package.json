{
  "name": "ucompress",
  "version": "0.8.1",
  "description": "A micro, all-in-one, compressor for common Web files",
  "main": "./cjs/index.js",
  "scripts": {
    "build": "npm run cjs && npm run test",
    "cjs": "ascjs --no-default esm cjs",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "test": "rm -rf test/dest && mkdir -p test/dest && nyc node test/index.js"
  },
  "keywords": [
    "minify",
    "compress",
    "html",
    "css",
    "svg",
    "png",
    "jpg",
    "gif"
  ],
  "author": "Andrea Giammarchi",
  "license": "ISC",
  "devDependencies": {
    "ascjs": "^4.0.0",
    "compression": "^1.7.4",
    "coveralls": "^3.0.14",
    "essential-md": "^0.3.0",
    "express": "^4.17.1",
    "nyc": "^15.0.1"
  },
  "module": "./esm/index.js",
  "type": "module",
  "exports": {
    "import": "./esm/index.js",
    "default": "./cjs/index.js"
  },
  "dependencies": {
    "csso": "^4.0.3",
    "gifsicle": "^5.0.0",
    "html-minifier": "^4.0.0",
    "jpegtran-bin": "^4.0.0",
    "mime-types": "^2.1.27",
    "pngquant-bin": "^5.0.2",
    "svgo": "^1.3.2",
    "uglify-es": "^3.3.9"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WebReflection/ucompress.git"
  },
  "bugs": {
    "url": "https://github.com/WebReflection/ucompress/issues"
  },
  "homepage": "https://github.com/WebReflection/ucompress#readme"
}
